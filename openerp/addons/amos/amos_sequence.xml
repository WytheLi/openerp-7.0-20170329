<?xml version="1.0" encoding="UTF-8" ?>
<openerp>
    <!--  定义序列  -->
    <!--  noupdate=0表示每次升级模块都更新  -->
    <data noupdate="1">
        <record id="seq_type_amos_text" model="ir.sequence.type">
            <field name="name">amos.text</field>
            <field name="code">amos.text</field>
        </record>

        <record id="seq_amos_text" model="ir.sequence">
            <field name="name">amos.text</field>
            <field name="code">amos.text</field>
            <field name="prefix">FK%(year)s%(month)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False" />
        </record>
    </data>
</openerp>